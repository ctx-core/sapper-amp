{
	"name": "@ctx-core/sapper-amp",
	"version": "4.4.3",
	"description": "@ctx-core Svelte Sapper with Google amp support",
	"homepage": "https://github.com/ctx-core/sapper-amp#readme",
	"scripts": {
		"build": "npm run clean && npm run compile && npm run symlink",
		"clean": "rm -f tsconfig.tsbuildinfo",
		"compile": "./node_modules/.bin/tsc -b .",
		"symlink": "./node_modules/.bin/symlink_src_assets_to_dist .",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"dependencies": {
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^7.0.10",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
