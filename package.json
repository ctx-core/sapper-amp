{
	"name": "@ctx-core/sapper-amp",
	"version": "5.2.6",
	"description": "@ctx-core Svelte Sapper with Google amp support",
	"homepage": "https://github.com/ctx-core/sapper-amp#readme",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./Amp_Layout"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"svelte": "^4.2.5"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.4",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/sapper-amp.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/sapper-amp/issues"
	},
	"sideEffects": false
}